{
    "name": "Youtube Spam/Bot Remover",
    "version": "1.0",
    "manifest_version": 3, 
    "permissions": [
        "tabs",
        "activeTab"
    ],  
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_title": "Youtube Spam Remover",  
        "default_popup": "popup.html"    
    }
}